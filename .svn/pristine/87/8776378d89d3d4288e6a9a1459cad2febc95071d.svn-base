		/* Flex-box spcf */
		display: flex;
		flex-direction: column;
		justify-content: space-between;

		/* Layout */
		width: 100%;
		min-height: calc(100vh);
		
		/* Test */
		background-color__: #e0e6e6

		
	.FlexiArea
		/* Flex-child spcf */
		order: 10;
		
		/* Flex-box spcf */
		display: flex;
		flex-direction: column;

		/* Layout */
